{
    "title": "iOS 文档",
    "author": "fanxiaocong",
    "description": "iOS IM SDK 文档",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "IM开发文档": "https://github.com/Eling10/im/tree/master"
        }
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "page-toc-button",
        "back-to-top-button",
        "-lunr", "-search", "search-pro",
        "lightbox",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy e-ling.com 2017",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD"
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "favicon":{
            "shortcut": "./source/images/icon.png",
            "bookmark": "./source/images/icon.png",
            "appleTouch": "./source/images/apple-touch-icon.jpg",
            "appleTouchMore": {
                "120x120": "./source/images/apple-touch-icon.jpg",
                "180x180": "./source/images/apple-touch-icon.jpg"
            }
        }
    }
}
